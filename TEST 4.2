#include <stdio.h>

int main() {
    int arr[] = {16, -18, 27, -16, 23, -21, 19};
    int size = sizeof(arr) / sizeof(arr[0]);
    int count = 0;

    for (int i = 0; i < size; i++) {
        if (arr[i] < 0) {
            count++;
        }
    }

    printf("Number of negative numbers in Array elements = %d\n", count);
    return 0;
}
