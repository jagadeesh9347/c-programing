#include <stdio.h>

int main() {
  int m, n, i, j;

  printf("Enter number of rows and columns of matrix: ");
  scanf("%d %d", &m, &n);

  int matrix[m][n];

  printf("Enter matrix elements:\n");

  // Read input matrix
  for(i = 0; i < m; i++) {
    for(j = 0; j < n; j++) {
      scanf("%d", &matrix[i][j]);
    }
  }

  // Transpose matrix
  int transpose[n][m];
  for(i = 0; i < n; i++) {
    for(j = 0; j < m; j++) {
      transpose[i][j] = matrix[j][i];
    }
  }

  // Output transpose
  printf("Transpose of the matrix:\n");
  for(i = 0; i < n; i++) {
    for(j = 0; j < m; j++) {
      printf("%d ", transpose[i][j]);
    }
    printf("\n");
  }

  return 0;
}
